        <rd:TypeName>System.String</rd:TypeName>
      </Field>
      <Field Name="PRLS001_CODE">
        <DataField>PRLS001_CODE</DataField>
        <rd:TypeName>System.String</rd:TypeName>
      </Field>
      <Field Name="EmpCode">
        <DataField>EmpCode</DataField>
        <rd:TypeName>System.String</rd:TypeName>
      </Field>
      <Field Name="ResumeDate">
        <DataField>ResumeDate</DataField>
        <rd:TypeName>System.DateTime</rd:TypeName>
      </Field>
      <Field Name="Actual_Leave_Type">
        <DataField>Actual_Leave_Type</DataField>
        <rd:TypeName>System.String</rd:TypeName>
      </Field>
      <Field Name="Duty_Rm_type">
        <DataField>Duty_Rm_type</DataField>
        <rd:TypeName>System.String</rd:TypeName>
      </Field>
      <Field Name="Approve_Days">
        <DataField>Approve_Days</DataField>
        <rd:TypeName>System.String</rd:TypeName>
      </Field>
      <Field Name="Excess_Days_plus_minus">
        <DataField>Excess_Days_plus_minus</DataField>
        <rd:TypeName>System.String</rd:TypeName>
      </Field>
      <Field Name="Approve_Days_in_full">
        <DataField>Approve_Days_in_full</DataField>
        <rd:TypeName>System.String</rd:TypeName>
