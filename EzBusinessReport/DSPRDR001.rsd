        <rd:TypeName>System.String</rd:TypeName>
      </Field>
      <Field Name="PRLR001_CODE">
        <DataField>PRLR001_CODE</DataField>
        <rd:TypeName>System.String</rd:TypeName>
      </Field>
      <Field Name="country">
        <DataField>country</DataField>
        <rd:TypeName>System.String</rd:TypeName>
      </Field>
      <Field Name="division">
