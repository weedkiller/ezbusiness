<?xml version="1.0" encoding="utf-8"?>
<SharedDataSet xmlns="http://schemas.microsoft.com/sqlserver/reporting/2010/01/shareddatasetdefinition" xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">
  <DataSet Name="DataSet3">
    <Query>
      <DataSourceReference>DataSource1</DataSourceReference>
      <DataSetParameters>
        <DataSetParameter Name="a.Cmpycode">
          <ReadOnly>false</ReadOnly>
          <Nullable>false</Nullable>
          <OmitFromQuery>false</OmitFromQuery>
          <rd:UserDefined>true</rd:UserDefined>
        </DataSetParameter>
        <DataSetParameter Name="a.PRDR001_CODE">
          <ReadOnly>false</ReadOnly>
          <Nullable>false</Nullable>
          <OmitFromQuery>false</OmitFromQuery>
          <rd:UserDefined>true</rd:UserDefined>
        </DataSetParameter>
      </DataSetParameters>
      <CommandText>SELECT        A.Cmpycode, A.PRDR001_CODE, A.PRLR001_CODE, A.country, A.division, A.PRLS001_CODE, A.EmpCode, A.ResumeDate, A.Actual_Leave_Type, A.Duty_Rm_type, A.Approve_Days, A.Excess_Days_plus_minus, 
                         A.Approve_Days_in_full, A.Approve_Days_in_Half, A.Flag, B.Empname, c.StartDate, c.EndDate, B.BalLeave
FROM            PRDR001 AS A INNER JOIN
                         MEM001 AS B ON A.EmpCode = B.EmpCode AND A.Cmpycode = B.Cmpycode INNER JOIN
                         PRLR001 AS c ON A.EmpCode = c.EmpCode AND A.Cmpycode = c.CmpyCode AND A.PRLR001_CODE = c.PRLR001_CODE</CommandText>
    </Query>
    <Fields>
      <Field Name="Cmpycode">
