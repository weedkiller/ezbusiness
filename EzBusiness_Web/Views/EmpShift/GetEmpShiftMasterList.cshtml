@model IEnumerable<EzBusiness_ViewModels.Models.Humanresourcepayroll.EmpShiftVM>


<script src="~/Scripts/jquery-3.1.1.js"></script>
<script src="~/Scripts/datatables/js/jquery.dataTables.min.js"></script>
<script src="~/Scripts/datatables/js/dataTables.bootstrap4.min.js"></script>

<script src="~/Scripts/EzComNDW.js"></script>
<script>
    $(document).ready(function () {
        Ezsidetbl1('#POList', '#POList tfoot td', false, "#POList thead");
    });
</script>

<table id="POList" class="table table-bordered">
    <thead>
        <tr class="tr">
            <th>Code</th>
            <th>Employee Code</th>
            <th>Shift Name Code</th>
            <th>Shift Allocation Code</th>
           
        </tr>
    </thead>
    <tfoot>
        <tr>
            <td>Code</td>
            <td>Employee Code</td>
            <td>Shift Name Code</td>
            <td>Shift Allocation Code</td>

        </tr>
    </tfoot>
    <tbody>
        @foreach (var po in Model)
        {
            <tr>
                <td>
                    @Html.Hidden("hdnprbmCode", po.PRSFT003_code)
                    @po.PRSFT003_code
                </td>
                <td>
                    @po.EmpCode
                </td>
                <td>
                    @po.PRSFT001_code
                </td>
                <td>
                    @po.PRSFT002_code
                </td>
               

            </tr>
        }
    </tbody>
</table>



