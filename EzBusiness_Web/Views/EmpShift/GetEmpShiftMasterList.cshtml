@model IEnumerable<EzBusiness_ViewModels.Models.Humanresourcepayroll.EmpShiftVM>



<script src="~/Content/assets/js/jquery.dataTables.min.js"></script>

<script src="~/Scripts/EzComNDW.js"></script>
<script>
        Ezsidetbl('#POList', '#POList tfoot th',false);       
</script>

<table id="POList" class="table table-bordered table-hover editlist">
    <thead>
        <tr>
            <th>Code</th>
            <th>Emp Code</th>
            <th>PRSFT001_code</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var po in Model)
        {
            <tr>
                <td>
                    @Html.Hidden("hdnprbmCode", po.PRSFT003_code)
                    @po.PRSFT003_code
                </td>
                <td>
                    @po.EmpCode
                </td>
                <td>
                    @po.PRSFT001_code
                </td>

            </tr>
        }
    </tbody>
</table>



