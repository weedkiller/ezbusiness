@model IEnumerable <EzBusiness_ViewModels.Models.Humanresourcepayroll.Finalsettalment_VM>


@{
    var a = 1;

}

<script src="~/Scripts/jquery-3.1.1.js"></script>
<script src="~/Scripts/datatables/js/jquery.dataTables.min.js"></script>
<script src="~/Scripts/datatables/js/dataTables.bootstrap4.min.js"></script>

<script src="~/Scripts/EzComNDW.js"></script>
<script>
    $(document).ready(function () {
        Ezsidetbl1('#POList', '#POList tfoot td',false, "#POList thead");
    });
</script>

<table id="POList" class="table table-bordered table-hover emptbl">
    <thead>
        <tr>
            <th>Code</th>
            <th>Employee Code</th>
            <th>Date</th>
        </tr>
    </thead>
    <tfoot>
        <tr>
            <td>Code</td>
            <td>Employee Code</td>
            <td>Date</td>
        </tr>
    </tfoot>
    <tbody>
        @foreach (var po in Model)
        {
            <tr>
                <td>
                    @Html.Hidden("hdnPRFSET001Code", po.PRFSET001_code)
                    @po.PRFSET001_code
                </td>
                <td>
                    @po.EmpCode
                </td>
                <td>
                  
                    @Convert.ToDateTime(@po.Dates).ToString("dd/MM/yyyy") 
                </td>

            </tr>
        }
    </tbody>
</table>