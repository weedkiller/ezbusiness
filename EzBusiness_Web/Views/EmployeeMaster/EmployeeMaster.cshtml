        function ValidateForm(employeemaster) {
           
            var response = {
                ErrorMessage: "",
                formValid: false
            };
            //if (employeemaster.IsEditMode == false) {
                if (employeemaster.Salutaion == "0") {
                    response.ErrorMessage += "Salutaion ,";
                }
