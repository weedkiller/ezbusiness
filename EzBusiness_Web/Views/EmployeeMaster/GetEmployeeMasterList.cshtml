@model IEnumerable<EzBusiness_ViewModels.Models.Humanresourcepayroll.Employee_VM>
    
@{
    var a = 1;

}

<script src="~/Content/assets/js/jquery.dataTables.min.js"></script>
<script src="~/Scripts/EzComNDW.js"></script>
    <script>
       
         
        Ezsidetbl("#POList", "#POList tfoot td", true,"#POList thead");
        //a++;
     
    </script>

<button class="btn btn-primary btn1 newbtn" id="btnNew"><span class="glyphicon glyphicon-plus-sign"></span>&nbsp;New</button>

<table id="POList" class="table table-bordered table-hover editlist">
    <thead>
        <tr>
            <th>Code</th>
            <th>Name</th>
            <th>
                Date Of Birth
            </th>
            <th>
                Contact No
            </th>
            <th>
                EMail Id
            </th>
            <th>
                Nationality
            </th>
            @*<th>
                Division Code
            </th>*@
            <th>
                Location Code
            </th>
            <th>
                VisaLocation Code
            </th>
            <th>
                Reporting Employee
            </th>
        </tr>
    </thead>
    <tfoot>
        <tr>
            <td>Code</td>
            <td>Name</td>
            <td>
                Date Of Birtd
            </td>
            <td>
                Contact No
            </td>
            <td>
                EMail Id
            </td>
            <td>
                Nationality
            </td>
            @*<td>
                    Division Code
                </td>*@
            <td>
                Location Code
            </td>
            <td>
                VisaLocation Code
            </td>
            <td>
                Reporting Employee
            </td>
        </tr>
    </tfoot>
    <tbody>
        @foreach (var po in Model)
        {
            <tr>
                <td>
                    @Html.Hidden("hdnMRCode", po.EmpCode)
                    @po.EmpCode
                </td>
                <td>
                    @po.Empname
                </td>
                <td>
                    @Convert.ToDateTime(@po.DOB).ToString("dd/MM/yyyy")
                </td>
                <td>
                    @po.ContactNo
                </td>
                <td>
                    @po.EMail
                </td>
                <td>
                    @po.Nationality
                </td>
                @*<td>
                    @po.DivisionCode
                </td>*@
                <td>
                    @po.LocCode
                </td>
                <td>
                    @po.VisaLocation
                </td>
                <td>
                    @po.ReportingEmp
                </td>

            </tr>
        }
    </tbody>

   
   
</table>
