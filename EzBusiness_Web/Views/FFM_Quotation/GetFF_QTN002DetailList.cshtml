@model IEnumerable<EzBusiness_ViewModels.Models.FreightManagement.SEA_Export.FF_QTN_VM>

<script src="~/Scripts/jquery-3.1.1.js"></script>
<script src="~/Scripts/datatables/js/jquery.dataTables.min.js"></script>
<script src="~/Scripts/datatables/js/dataTables.bootstrap4.min.js"></script>
<script src="~/Scripts/EzComNDW.js"></script>
<script>
   Ezsidetbl("#POList", "#POList tfoot td", false, "#POList thead");        
</script>


<table id="POList" class="table table-bordered stripe display hover portal-drivers">
    <thead>
        <tr class="tr">
            <th>QTN Code</th>
            <th>Cust Code</th>
            <th>Contact</th>
            <th>Telephone</th>
            <th>Email</th>
            <th>Customer_Ref</th>
            <th>Pickup Place</th>
            <th>POL</th>
            <th>POD</th>
            <th>FND</th>
            <th>MOVE_TYPE</th>
            <th>REF No</th>
            <th>Vessal</th>
            <th>Voyage</th>           
        </tr>
    </thead>
    <tfoot>
        <tr>
            <td>QTN Code</td>
            <td>Cust Code</td>
            <td>Contact</td>
            <td>Telephone</td>
            <td>Email</td>
            <td>Customer_Ref</td>
            <td>Pickup Place</td>
            <td>POL</td>
            <td>POD</td>
            <td>FND</td>
            <td>MOVE_TYPE</td>
            <td>REF No</td>
            <td>Vessal</td>
            <td>Voyage</td>       
        </tr>
    </tfoot>
    <tbody>
        @foreach (var po in Model)
        {
            <tr>
                <td>
                    @Html.Hidden("hdnMRCode", po.FF_QTN001_CODE)
                    @po.FF_QTN001_CODE
                </td>
                <td>
                    @po.CUST_CODE
                </td>
                <td>
                   @po.CONTACT
                </td>
                <td>
                    @po.TELEPHONE
                </td>
                <td>
                    @po.EMAIL
                </td>
                <td>
                    @po.CUSTOMER_REF
                </td>
                <td>
                    @Html.Hidden("hdnDivisionCode", po.PICKUP_PLACE)
                    @po.PICKUP_PLACE
                </td>
                <td>
                    @po.POL
                </td>
                <td>
                    @po.POD
                </td>
                <td>
                    @po.FND
                </td>
                <td>
                    @po.MOVE_TYPE
                </td>
                <td>
                    @po.REF_NO
                </td>
                <td>
                    @po.VESSEL
                </td>
                <td>
                    @po.VOYAGE
                </td>
            </tr>
        }
    </tbody>
</table>
               
