            $("#tablePODetails5 > tbody  > tr").each(function () {
                var amount = parseFloat($(this).find("[name*='txtVend_Net_Amt']").val());
                var amount1 = parseFloat($(this).find("[name*='txtCust_Net_Amt']").val());
                sum += isNaN(amount) ? 0 : amount;
                sum1 += isNaN(amount1) ? 0 : amount1;
            });
            $("[name*='Total_Billed']").val('' + sum);
            $("[name*='Total_Cost']").val('' + sum1);
            $("[name*='Total_Profit']").val('' + (sum1 - sum));
