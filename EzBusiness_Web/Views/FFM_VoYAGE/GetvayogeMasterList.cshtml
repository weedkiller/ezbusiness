@model IEnumerable<EzBusiness_ViewModels.Models.FreightManagement.FFM_VOYAGE_VM>

@{
  
    var counter = 1;
}


<br />
<script src="~/Scripts/jquery-3.1.1.js"></script>
<script src="~/Scripts/datatables/js/jquery.dataTables.min.js"></script>
<script src="~/Scripts/datatables/js/dataTables.bootstrap4.min.js"></script>
<script src="~/Scripts/EzComNDW.js"></script>
@*<script src="~/Scripts/Application/FrightManagment/FFM_Voyage.js"></script>*@
@*<script src="~/Scripts/Application/FrightManagment/FMM_CNTRFright.js"></script>*@
<script>
    Ezsidetbl("#tblUnits", "#tblUnits tfoot td", false, "#tblUnits thead");
</script>

<div class="table-responsive">
      <table class="table table-bordered stripe display hover" id="tblUnits">
                    <thead>
                        <tr class="tr">
                            <th>
                                Vessale CODE
                            </th>
                            <th>
                                Vessale Name
                            </th>
                            <th>
                                VoYage Code
                            </th>
                          
                        </tr>
                    </thead>
                    <tfoot>
                        <tr class="tr">
                            <td>
                                @Html.DisplayNameFor(model => model.FFM_VESSEL_CODE)

                            <td>
                                @Html.DisplayNameFor(model => model.NAME)
                            </td>
                            <td>
                                @Html.DisplayNameFor(model => model.FFM_VOYAGE01_CODE)
                            </td>
                           
                        </tr>
                    </tfoot>
                    <tbody>
                        @foreach (var item in Model)
                        {
                            <tr>

                                <td>
                                    @Html.DisplayFor(modelItem => item.FFM_VESSEL_CODE)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.NAME)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.FFM_VOYAGE01_CODE)
                                    @Html.Hidden("hdnFNMBRANCH_CODE", item.FFM_VOYAGE01_CODE)
                                </td>
                               
                            </tr>
                            counter++;
                        }
                    </tbody>

                </table>
        @Html.Hidden("Counter", counter)
   
</div>




