@model IEnumerable<EzBusiness_ViewModels.Models.Humanresourcepayroll.ShiftMasterVM>

<div class="col-md-12 portal-drivers">
    <table id="POList" class="table table-bordered emptbl">
        <thead>
            <tr class="tr">
                <th>Code</th>
                <th>Shift Name</th>
                <th>Country</th>

            </tr>
        </thead>
        <tfoot>
            <tr>
                <td>Code</td>
                <td>Shift Name</td>
                <td>Country</td>

            </tr>
        </tfoot>
        <tbody>
            @foreach (var po in Model)
        {
                <tr>
                    <td>
                        @Html.Hidden("hdnprbmCode", po.PRSFT001_code)
                        @po.PRSFT001_code
                    </td>
                    <td>
                        @po.ShiftName
                    </td>
                    <td>
                        @po.country
                    </td>


                </tr>
        }
        </tbody>
    </table>
</div>
<script src="~/Scripts/jquery-3.1.1.js"></script>
<script src="~/Scripts/datatables/js/jquery.dataTables.min.js"></script>
<script src="~/Scripts/datatables/js/dataTables.bootstrap4.min.js"></script>
<script src="~/Scripts/EzComNDW.js"></script>
