@model IEnumerable<EzBusiness_ViewModels.Models.Humanresourcepayroll.ShiftMasterVM>
<script src="~/Content/assets/js/jquery.dataTables.min.js"></script>
<script src="~/Scripts/EzComNDW.js"></script>
<script>
    Ezsidetbl("#POList", "#POList tfoot th", false);
</script>

<table id="POList" class="table table-bordered table-hover editlist">
    <thead>
        <tr>
            <th>Code</th>
            <th>Shift Name</th>
            <th>Country</th>
         
        </tr>
    </thead>
    <tbody>
        @foreach (var po in Model)
        {
            <tr>
                <td>
                    @Html.Hidden("hdnprbmCode", po.PRSFT001_code)
                    @po.PRSFT001_code
                </td>
                <td>
                    @po.ShiftName
                </td>
                <td>
                    @po.country
                </td>
                

            </tr>
        }
    </tbody>
</table>
