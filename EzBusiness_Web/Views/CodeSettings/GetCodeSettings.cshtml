@model IEnumerable<EzBusiness_ViewModels.Models.FreightManagement.UTM0001_VM>

<script src="~/Scripts/jquery-3.1.1.js"></script>
<script src="~/Scripts/datatables/js/jquery.dataTables.min.js"></script>
<script src="~/Scripts/datatables/js/dataTables.bootstrap4.min.js"></script>
<script src="~/Scripts/EzComNDW.js"></script>
<script>
    Ezsidetblnew("#POList", "#POList tfoot td", false, "#POList thead");
</script>


<table id="POList" class="table table-bordered stripe display hover portal-drivers">

    <thead>
        <tr class="tr">   
            <th>Code</th>                   
            <th>Table Name</th>
            <th>Module Type</th>            
            <th>Prefix</th>
            <th>Page Name</th>
            <th>Starting No</th>
            <th>Total Length</th>
            <th>Last No</th>
            <th>Auto</th>
</tr>
    </thead>
    <tfoot>
        <tr>
            <td>Code</td>
            <td>Table Name</td>
            <td>Module Type</td>           
            <td>Prefix</td>
            <td>Page Name</td>
            <td>Starting No</td>
            <td>Total Length</td>
            <td>Last No</td>
            <td>Auto</td>
        </tr>
    </tfoot>
    <tbody>
        @foreach (var po in Model)
        {
            <tr>
               
                <td>@po.UTI0001_CODE
                    @Html.Hidden("hdnUTI0001_CODE", po.UTI0001_CODE)
                </td>
                <td> @po.Tablename</td>  
                <td>@po.Module_Type</td>               
                <td>@po.PREFIX_CODE</td>
                <td>@po.Page_Name</td>
                <td>@po.Starting_No</td>
                <td>@po.Total_length</td>
                <td> @po.Last_No</td>
                <td>@po.Auto_increment</td>
            </tr>
        }
    </tbody>
</table>
