@model IEnumerable<EzBusiness_ViewModels.Models.FreightManagement.UTM0001_VM>

<script src="~/Scripts/jquery-3.1.1.js"></script>
<script src="~/Scripts/datatables/js/jquery.dataTables.min.js"></script>
<script src="~/Scripts/datatables/js/dataTables.bootstrap4.min.js"></script>
<script src="~/Scripts/EzComNDW.js"></script>
<script>
    Ezsidetblnew("#POList", "#POList tfoot td", false, "#POList thead");
</script>


<table id="POList" class="table table-bordered stripe display hover portal-drivers">

    <thead>
        <tr class="tr">                      
            <th>Table Name</th>
            <th>Code</th>
            <th>Prefix</th>
            <th>Page Name</th>
            <th>Starting No</th>
            <th>Total Length</th>
            <th>Last No</th>
            <th>Auto</th>
</tr>
    </thead>
    <tfoot>
        <tr>
            <td>Table Name</td>
            <td>Code</td>
            <td>Prefix</td>
            <td>Page Name</td>
            <td>Starting No</td>
            <td>Total Length</td>
            <td>Last No</td>
            <td>Auto</td>
        </tr>
    </tfoot>
    <tbody>
        @foreach (var po in Model)
        {
            <tr>
                <td>
                    @Html.Hidden("hdnTablename", po.Tablename)
                    @po.Tablename
                </td>               
                <td>@po.UTI0001_CODE</td>
                <td>@po.PREFIX_CODE</td>
                <td>@po.Page_Name</td>
                <td>@po.Starting_No</td>
                <td>@po.Total_length</td>
                <td> @po.Last_No</td>
                <td>@po.Auto_increment</td>
            </tr>
        }
    </tbody>
</table>
