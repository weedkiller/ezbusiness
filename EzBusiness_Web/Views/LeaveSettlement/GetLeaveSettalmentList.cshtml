@model IEnumerable<EzBusiness_ViewModels.Models.Humanresourcepayroll.LeaveSettlementVM>
<script src="~/Scripts/jquery-3.1.1.js"></script>
<script src="~/Scripts/datatables/js/jquery.dataTables.min.js"></script>
<script src="~/Scripts/datatables/js/dataTables.bootstrap4.min.js"></script>
<script src="~/Scripts/EzComNDW.js"></script>

<script>
    $(document).ready(function () {
        Ezsidetbl1('#LVList', '#LVList tfoot td', true, "#LVList thead");
    });
</script>


<table id="LVList" class="table table-bordered table-hover editlist">
    <thead>
        <tr>
            <th>LANo</th>
            <th>Emp Code</th>
            <th>LS Date</th>
            <th>Entry Date</th>


        </tr>
    </thead>
    <tfoot>
        <tr>
            <td>LANo</td>
            <td>Emp Code</td>
            <td>LS Date</td>
            <td>Entry Date</td>


        </tr>
    </tfoot>
    <tbody>
        @foreach (var sm in Model)
        {
            <tr>
                <td>
                    @Html.Hidden("hdnPRSR001_CODE", sm.PRLS001_CODE)
                    @sm.PRLS001_CODE
                </td>
                <td>
                    @sm.Empcode
                </td>
                <td>
                    @sm.LLSdate
                </td>
                <td>
                    @sm.Entry_Date
                </td>

            </tr>
        }
    </tbody>
</table>