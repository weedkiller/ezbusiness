@model  IEnumerable < EzBusiness_ViewModels.Models.MenuItem.MenuItemRUVM>



<script src="~/Content/assets/js/jquery.dataTables.min.js"></script>

<script src="~/Scripts/EzComNDW.js"></script>
<script>
    Ezsidetbl('#LVList', '#LVList tfoot th', false);
</script>

<table id="LVList" class="table table-bordered table-hover editlist">
    <thead>
        <tr>
            <th>UserName</th>
            <th>Employee Code</th>
            <th>User Type</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var po in Model)
        {
            <tr>
                <td>
                    @Html.Hidden("hdnuser_name", po.user_name)
                    @po.user_name
                </td>
                <td>
                    @po.EmpCode
                </td>
                <td>
                    @po.Utype
                </td>

            </tr>
        }
    </tbody>
</table>


