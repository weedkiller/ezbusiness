@model  IEnumerable < EzBusiness_ViewModels.Models.MenuItem.MenuItemRUVM>



<script src="~/Scripts/jquery-3.1.1.js"></script>
<script src="~/Scripts/datatables/js/jquery.dataTables.min.js"></script>

<script src="~/Scripts/datatables/js/dataTables.bootstrap4.min.js"></script>

<script src="~/Scripts/EzComNDW.js"></script>
<script>
    Ezsidetblnew('#LVList', '#LVList tfoot td', false, "#LVList thead");
</script>

<div class="col-md-12 portal-drivers">
    <table id="LVList" class="table table-bordered stripe display hover emptbl">
        <thead>
            <tr class="tr">
                <th>UserName</th>
                <th>Employee Code</th>
                <th>User Type</th>
            </tr>
        </thead>
        <tfoot>
            <tr>
                <td>UserName</td>
                <td>Employee Code</td>
                <td>User Type</td>
            </tr>
        </tfoot>

        <tbody>
            @foreach (var po in Model)
        {
        <tr>
            <td>
                @Html.Hidden("hdnuser_name", po.user_name)
                @po.user_name
            </td>
            <td>
                @po.EmpCode
            </td>
            <td>
                @po.Utype
            </td>

        </tr>
        }
        </tbody>
    </table>
</div>

