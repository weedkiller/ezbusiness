@model IEnumerable<EzBusiness_ViewModels.Models.FreightManagement.FNMBranch_VM>
@{
    ViewBag.Title = "FNMBranch";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<script src="~/Scripts/EzComNDW.js"></script>
<script src="~/Scripts/Application/FNMBranchMasterJS.js"></script>

<div class="row">
    <div class="col-md-6 col-sm-6 col-xs-6 btngrp">

        <div class="title-action">
            <button class="btn btn-primary btn1 newbtn" id="btnNew"><span class="glyphicon glyphicon-plus-sign"></span>&nbsp;New</button>
            <button type="submit" class="btn btn-primary btn1" id="btnSave" style="display:none"><span class="glyphicon glyphicon-floppy-disk"></span>&nbsp; Save</button>
            <button class="btn btn-warning btn1" id="btnCancel" style="display:none"><span class="glyphicon glyphicon-off"></span>&nbsp; Cancel</button>
            <button class="btn btn-danger btn1" id="btnDelete" style="display:none"><span class="glyphicon glyphicon-remove"></span>&nbsp; Delete</button>
            <button class="btn btn-info btn1" id="btnPrint" style="display:none"><span class="glyphicon glyphicon-print"></span>&nbsp; Print</button>
            <input type="hidden" id="hdnOperationMode" value="" />
        </div>
    </div>

</div><hr />
<!-- Row end -->
<!-- Row start -->
<div class="row" id="showlist">
    <!--updated code-->

    <div class="col-md-12 no-padding navbar-expand-lg">
        <div class="panel panel-primary panl">
            <div class="panel-heading clearfix">
                <i class="icon-calendar"></i>
                <h3 class="panel-title">Ez Business System</h3>
            </div>
            <div class="panel-body" style="margin-left:10px;">
                <label class="row" id="ErrorMessage" style="display:none"></label>
                <div class="tabbable-line boxless tabbable-reversed" id="DRContainer">
                    <div class="custom-form-wrapper">
                        <div id="DRContainer">
                            <div class="row">
                                <div class="custom-form-wrapper">
                                    <!-- BEGIN FORM-->
                                    <div class="form-horizontal">
                                        <div class="form-body">
                                            <div class="container" id="DRListContainer">                                             
                                                <div class="col-md-12 portal-drivers">
                                                    <table id="DRList" class="table table-bordered table-hover">
                                                        <thead>
                                                            <tr class="tr">
                                                                <th>FNMBRANCH CODE</th>
                                                                <th>EMAIL</th>
                                                                <th>ADDRESS</th>
                                                                <th>MOBILE</th>
                                                                <th>PRINTNAME</th>
                                                            </tr>
                                                        </thead>
                                                        @*<tfoot>
                                                            <tr>
                                                                <th>FNMBRANCH CODE</th>
                                                                <th>EMAIL</th>
                                                                <th>ADDRESS</th>
                                                                <th>MOBILE</th>
                                                                <th>PRINTNAME</th>
                                                            </tr>
                                                        </tfoot>*@
                                                        <tbody>
                                                            @foreach (var sm in Model)
                                                            {
                                                                <tr>
                                                                    <td>
                                                                        @Html.DisplayFor(modelItem => sm.FNMBRANCH_CODE)
                                                                        @Html.Hidden("hdnFNMBRANCH_CODE", sm.FNMBRANCH_CODE)
                                                                    </td>
                                                                    <td>
                                                                       @Html.DisplayFor(modelItem => sm.EMAIL)
                                                                    </td>
                                                                    <td>@Html.DisplayFor(modelItem => sm.ADDRESS)</td>
                                                                    <td>
                                                                       @Html.DisplayFor(modelItem => sm.MOBILE)
                                                                    </td>
                                                                    <td>
                                                                       @Html.DisplayFor(modelItem => sm.PRINTNAME)
                                                                    </td>
                                                                </tr>
                                                            }
                                                        </tbody>
                                                    </table>
                                                </div>


                                            </div>

                                        </div>
                                    </div><br />
                                    <!-- END FORM-->

                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>


<div class="row" id="shownewbtn" style="display:none">
    <div class="col-md-12 no-padding navbar-expand-lg">           
            <div class="panel panel-primary panl">
                <div class="panel-heading clearfix">
                    <i class="icon-calendar"></i>
                    <h3 class="panel-title">Ez Business System</h3>
                </div>
                <div class="panel-body" style="margin-left:10px;">
                    <div class="tabbable-line boxless tabbable-reversed" id="DRContainer">
                        <div class="custom-form-wrapper">
                            <div id="DRContainer">
                                <div class="row">
                                    <div class="custom-form-wrapper">
                                        <!-- BEGIN FORM-->
                                        <div class="form-horizontal">
                                            <div class="form-body">
                                                <div class="row">
                                                    <div class="col-md-6">
                                                        <div class="form-group">
                                                            <label class="control-label col-md-3">FNMBranch Code<i class="star">*</i></label>
                                                            <div class="col-md-9 input-field">
                                                                <input type="text" class="form-control" id="branchcode" placeholder="FNMBranchCode" />
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <!--/span-->
                                                    <div class="col-md-6">
                                                        <div class="form-group">
                                                            <label class="control-label col-md-3">Print Name</label>
                                                            <div class="col-md-9 input-field">
                                                                <input type="text" class="form-control" id="PrintName" placeholder="Print Name" />
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <!--/span-->
                                                </div>
                                                <!--/row-->
                                                <div class="row">
                                                    <div class="col-md-6">
                                                        <div class="form-group">
                                                            <label class="control-label col-md-3">Email</label>
                                                            <div class="col-md-9 input-field">
                                                                <input type="text" class="form-control" id="Email" placeholder="Email" />
                                                                @*@Html.HiddenFor(m => m.StartDate)*@
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <!--/span-->
                                                    <div class="col-md-6">
                                                        <div class="form-group">
                                                            <label class="control-label col-md-3">Mobile Number</label>
                                                            <div class="col-md-9 input-field">
                                                                <input type="text" class="form-control" id="mobiletxt" placeholder="MOBILE" />
                                                                @*@Html.HiddenFor(m => m.EndDate)*@
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <!--/span-->
                                                </div>



                                                <div class="row">
                                                    <div class="col-md-6">
                                                        <div class="form-group">
                                                            <label class="control-label col-md-3">WebSite</label>
                                                            <div class="col-md-9 input-field">
                                                                <input type="text" class="form-control" id="WebSitetxt" placeholder="WebSite" />
                                                            </div>
                                                        </div>
                                                    </div>

                                                    <div class="col-md-6">
                                                        <div class="form-group">
                                                            <label class="control-label col-md-3">Currency</label>
                                                            <div class="col-md-9 input-field">
                                                                <input type="text" class="form-control" id="Currencytxt" placeholder="Currency" />
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="row">
                                                    <div class="col-md-6">
                                                        <div class="form-group">
                                                            <label class="control-label col-md-3">Country</label>
                                                            <div class="col-md-9 input-field">
                                                                <input type="text" class="form-control" id="Countrytxt" placeholder="Country" />

                                                            </div>
                                                        </div>
                                                    </div>

                                                    <div class="col-md-6">
                                                        <div class="form-group">
                                                            <label class="control-label col-md-3">State</label>
                                                            <div class="col-md-9 input-field">
                                                                <input type="text" class="form-control" id="Statetxt" placeholder="State" />

                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="row">
                                                    <div class="col-md-6">
                                                        <div class="form-group">
                                                            <label class="control-label col-md-3">Address</label>
                                                            <div class="col-md-9 input-field">
                                                                <input type="text" class="form-control" id="Addresstxt" placeholder="Address" />

                                                            </div>
                                                        </div>

                                                    </div>

                                                    <div class="col-md-6">
                                                        <div class="form-group">
                                                            <label class="control-label col-md-3">Description</label>
                                                            <div class="col-md-9 input-field">
                                                                <input type="text" class="form-control" id="Descriptiontxt" placeholder="Description" />

                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div><br />
                                            <!-- END FORM-->
                                        </div>
                                    </div>
                                </div>
                            </div>

                        </div>
                    </div>

                </div>
    </div>

</div>
</div>

<script type="text/javascript">
        $(document).ready(function () {

           // EzdteformtcurRep('#fnlfdatetxt,#fnltdatetxt', 'DD/MM/YYYY');

            FNMB.initialize();
            });
</script>





