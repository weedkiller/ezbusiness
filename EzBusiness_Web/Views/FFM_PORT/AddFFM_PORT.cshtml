<!-- Row start -->
<div class="row">
    <div class="col-md-12">
        <div class="tabbable-line boxless tabbable-reversed" id="POContainer">
            <div class="custom-form-wrapper">
                <div id="POContainer">
                    <div class="row">
                        <div class="custom-form-wrapper">
                            <!-- BEGIN FORM-->
                            <div class="form-horizontal">
                                <div class="form-body">
                                    <div class="row">
                                        <div class="col-md-6">
                                            <div class="form-group">
                                                <label class="control-label col-md-4">Port Code</label>
                                                <div class="col-md-8 input-field">
                                                    @Html.TextBoxFor(m => m.FFM_PORT_CODE, new { @class = "form-control", @placeholder = "Code" })
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col-md-6">
                                            <div class="form-group">
                                                <label class="control-label col-md-4">Name</label>
                                                <div class="col-md-8 input-field">
                                                    @Html.TextBoxFor(m => m.NAME, new { @class = "form-control" })
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col-md-6">
                                            <div class="form-group">
                                                <label class="control-label col-md-4">Country</label>
                                                <div class="col-md-8 input-field">
                                                    @Html.DropDownListFor(m => m.COUNTRY, Model.CountryList, new { @class = "selectpicker form-control", @data_live_search = "true" })
                                                    @*<input id="COUNTRYtbl" type="text" class="selectpicker form-control"/>*@
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col-md-6">
                                            <div class="form-group">
                                                <label class="control-label col-md-4">Terminal</label>
                                                <div class="col-md-8 input-field">
                                                    @Html.TextBoxFor(m => m.TERMINAL, new { @class = "form-control" })
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col-md-6">
                                            <div class="form-group">
                                                <label class="control-label col-md-4">Langitude</label>
                                                <div class="col-md-8 input-field">
                                                    @Html.TextBoxFor(m => m.LANGITUDE, new { @class = "form-control" })
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col-md-6">
                                            <div class="form-group">
                                                <label class="control-label col-md-4">Latitude</label>
                                                <div class="col-md-8 input-field">
                                                    @Html.TextBoxFor(m => m.LATITUDE, new { @class = "form-control" })
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col-md-6">
                                            <div class="form-group">
                                                <label class="control-label col-md-4">Port Type</label>
                                                <div class="col-md-8 input-field">                                                   
                                                        @{
                                                            var TypeListdata = new SelectList(new[]
                                                                     {
                                                        new SelectListItem { Text = "AIR", Value = "AIR" },
                                                        new SelectListItem { Text = "SEA", Value = "SEA"},
                                                         }, "Value", "Text");
                                                        }
                                                        @Html.DropDownListFor(m => m.PortType, TypeListdata, new { @class = "form-control selectpicker" })
                                                    </div>
                                               </div>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col-md-6">
                                            <div class="form-group">
                                                <label class="control-label col-md-4">Display Status </label>
                                                <div class="col-md-8 input-field">
                                                    @{
                                                        var TypeList = new SelectList(new[]
                                                                 {
                                                        new SelectListItem { Text = "YES", Value = "Y" },
                                                        new SelectListItem { Text = "NO", Value = "N"},
                                                         }, "Value", "Text");
                                                    }
                                                    @Html.DropDownListFor(m => m.DISPLY_STATUS, TypeList, new { @class = "form-control selectpicker" })
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>



   



