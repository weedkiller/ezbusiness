<script src="~/Scripts/jquery-3.1.1.js"></script>
<script src="~/Scripts/moment.min.js"></script>
<script src="~/Scripts/datatables/js/jquery.dataTables.min.js"></script>
<script src="~/Scripts/datatables/js/dataTables.bootstrap4.min.js"></script>
<script src="~/Scripts/bootstrap-select.min.js"></script>
<link href="~/Content/bootstrap-select.min.css" rel="stylesheet" />
<script src="~/Scripts/bootstrap-datetimepicker.min.js"></script>
<script src="~/Scripts/EzComNDW.js"></script>
<script src="~/Scripts/jquery-ui-1.12.1.min.js"></script>
<link href="~/Styles/jquery-ui.css" rel="stylesheet" />

<script>
    $('.selectpicker').selectpicker('refresh');
    $("#NAME").on("focus", function () {
        $("#COUNTRY").removeClass('selectpicker');
        EzDropFill("COUNTRY", "/FFM_PORT/GetCountry1");
    });

    $("#COUNTRY").on("click", function () {
        debugger;
        $("#COUNTRY").removeClass('selectpicker');
        EzDropFill("COUNTRY", "/FFM_PORT/GetCountry1");
    });
</script>
<input id="hfCustomer" type="hidden">
<div class="tabbable-line boxless tabbable-reversed" id="POContainer">
    <div class="custom-form-wrapper">
        <div id="POContainer">
            <div class="row">
                <div class="custom-form-wrapper">
                    <!-- BEGIN FORM-->
                    <div class="container" id="POListContainer">
                        @*@Html.Action("GetInvtList", "SalesInovice")*@
                    </div>
                </div>
            </div>

        </div>
    </div>
</div>
<!-- Row start -->
<div class="row">
    <div class="col-md-12">
        <div class="tabbable-line boxless tabbable-reversed" id="POContainer">
            <div class="custom-form-wrapper">
                <div id="POContainer">
                    <div class="row">
                        
                        @*English Text: <input size="40" type="text" id="txtEnglish" /> <br />
                        Hindi Text`enter code here` : <input size="40" type="text" id="txtHindi" />*@

                        <div class="custom-form-wrapper">
                            <!-- BEGIN FORM-->
                            <div class="form-horizontal">
                                <div class="form-body">
                                    <div class="row">
                                        <div class="col-md-6">
                                            <div class="form-group">
                                                <label class="control-label col-md-4">Port Code</label>
                                                <div class="col-md-8 input-field">
                                                    @Html.TextBoxFor(m => m.FFM_PORT_CODE, new { @class = "form-control", @placeholder = "Code" })
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col-md-6">
                                            <div class="form-group">
                                                <label class="control-label col-md-4">Name</label>
                                                <div class="col-md-8 input-field">
                                                    @Html.TextBoxFor(m => m.NAME, new { @class = "form-control" })
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col-md-6">
                                            <div class="form-group">
                                                <label class="control-label col-md-4">Country</label>
                                                <div class="col-md-8 input-field">
                                                    @{
                                                        var TypeListdata1 = new SelectList(new[]
                                                        {
                                                        new SelectListItem { Text = "-Select-", Value = "-1" },                                                       
                                                         }, "Value", "Text");
                                                    }
                                                    @Html.DropDownListFor(m => m.COUNTRY, TypeListdata1, new { @class = "form-control", @data_live_search = "true" })                                                   
                                            </div>
                                        </div>
                                    </div>
                                        </div>
                                    <div class="row">
                                        <div class="col-md-6">
                                            <div class="form-group">
                                                <label class="control-label col-md-4">Terminal</label>
                                                <div class="col-md-8 input-field">
                                                    @Html.TextBoxFor(m => m.TERMINAL, new { @class = "form-control" })
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col-md-6">
                                            <div class="form-group">
                                                <label class="control-label col-md-4">Langitude</label>
                                                <div class="col-md-8 input-field">
                                                    @Html.TextBoxFor(m => m.LANGITUDE, new { @class = "form-control" })
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col-md-6">
                                            <div class="form-group">
                                                <label class="control-label col-md-4">Latitude</label>
                                                <div class="col-md-8 input-field">
                                                    @Html.TextBoxFor(m => m.LATITUDE, new { @class = "form-control" })
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col-md-6">
                                            <div class="form-group">
                                                <label class="control-label col-md-4">Port Type</label>
                                                <div class="col-md-8 input-field">                                                   
                                                        @{
                                                            var TypeListdata = new SelectList(new[]
                                                                     {
                                                        new SelectListItem { Text = "AIR", Value = "AIR" },
                                                        new SelectListItem { Text = "SEA", Value = "SEA"},
                                                         }, "Value", "Text");
                                                        }
                                                        @Html.DropDownListFor(m => m.PortType, TypeListdata, new { @class = "selectpicker form-control", @data_live_search = "true" })
                                                    </div>
                                               </div>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col-md-6">
                                            <div class="form-group">
                                                <label class="control-label col-md-4">Display Status </label>
                                                <div class="col-md-8 input-field">
                                                    @{
                                                        var TypeList = new SelectList(new[]
                                                                 {
                                                        new SelectListItem { Text = "YES", Value = "Y" },
                                                        new SelectListItem { Text = "NO", Value = "N"},
                                                         }, "Value", "Text");
                                                    }
                                                    @Html.DropDownListFor(m => m.DISPLY_STATUS, TypeList, new { @class = "selectpicker form-control", @data_live_search = "true" })
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>


