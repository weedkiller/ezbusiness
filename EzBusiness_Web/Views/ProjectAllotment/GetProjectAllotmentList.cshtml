@model IEnumerable<EzBusiness_ViewModels.Models.Humanresourcepayroll.ProjectAllotmentVM>



<script src="~/Scripts/jquery-3.1.1.js"></script>
<script src="~/Scripts/datatables/js/jquery.dataTables.min.js"></script>
<script src="~/Scripts/datatables/js/dataTables.bootstrap4.min.js"></script>

<script src="~/Scripts/EzComNDW.js"></script>
<script>
    Ezsidetbl1('#POList', '#POList tfoot td', false, "#POList thead");
</script>

<div class="col-md-12 portal-drivers">
<table id="POList" class="table table-bordered">
    <thead>
        <tr class="tr">
            <th>Code</th>
            <th>Employee Code</th>
            <th>Project Code</th>
        </tr>
    </thead>
    <tfoot>
        <tr>
            <td>Code</td>
            <td>Employee Code</td>
            <td>CCH004_code</td>
        </tr>
    </tfoot>
    <tbody>
        @foreach (var po in Model)
        {
            <tr>
                <td>
                    @Html.Hidden("hdnPRPRJE001code", po.PRPRJE001_code)
                    @po.PRPRJE001_code
                </td>
                <td>
                    @po.EmpCode
                </td>
                <td>
                    @po.CCH004_code
                </td>

            </tr>
        }
    </tbody>
</table>
</div>



