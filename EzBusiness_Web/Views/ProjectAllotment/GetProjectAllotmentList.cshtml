@model IEnumerable<EzBusiness_ViewModels.Models.Humanresourcepayroll.ProjectAllotmentVM>



<script src="~/Content/assets/js/jquery.dataTables.min.js"></script>

<script src="~/Scripts/EzComNDW.js"></script>
<script>
        Ezsidetbl('#POList', '#POList tfoot th',false);
</script>

<table id="POList" class="table table-bordered table-hover editlist">
    <thead>
        <tr>
            <th>Code</th>
            <th>Emp Code</th>
            <th>CCH004_code</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var po in Model)
        {
            <tr>
                <td>
                    @Html.Hidden("hdnPRPRJE001code", po.PRPRJE001_code)
                    @po.PRPRJE001_code
                </td>
                <td>
                    @po.EmpCode
                </td>
                <td>
                    @po.CCH004_code
                </td>

            </tr>
        }
    </tbody>
</table>




